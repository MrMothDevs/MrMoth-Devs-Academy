      <!-- Butoni i dark mode -->
      <script>
         function lol() {
             let element = document.body;
             element.classList.toggle("dark-mode");
           }
      </script>
      
      <title>MDA - MrMoth Devs Academy</title>
      
      <!-- Kodi per navbarin ne scroll -->
      <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
      <script src="js/navscroll.js"></script>

   </head>
   <body>
     
    <!-- Preloader per faqen -->
      <div class="loader-container">
         <div class="loader-content">
            <div class="loader"></div>
            <div class="loader-message">
               <span>Loading...</span>
            </div>
         </div>
      </div>

      <!-- Buton qe te con ne fillim te faqes (start)-->
      <button onclick="topFunction()" id="scrollBtn" title="Go to top" class="scrollBtn">
         <svg width="1em" height="1em" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
            <path d="M12 10.545L7.176 15.37a1.86 1.86 0 11-2.631-2.631l6.077-6.077a1.949 1.949 0 012.756 0l6.077 6.077a1.86 1.86 0 11-2.631 2.63L12 10.546z" fill="currentcolor" fill-rule="evenodd"></path>
         </svg>
      </button>

      <!-- Navigimi (Start)-->
      <header>
         <nav id="navbar" class="navbar">
            <img src="images/mda.png" alt="MDA logo" class="logo">
            <ul class="menu">
               <li class="nav-item">
                  <a href="/" class="nav-link hvr-underline-from-center" rel="noopener">Home</a>
               </li>
               <li class="nav-item">
                  <a href="courses" class="nav-link hvr-underline-from-center" rel="noopener">Courses</a>
               </li>
               <li class="nav-item">
                  <a href="contact" class="nav-link hvr-underline-from-center" rel="noopener">Contact Us</a>
               </li>
               <li class="nav-item">
                  <a href="signup" role="button" class="signup-btn" rel="noopener">Sign Up</a>
               </li>
            </ul>
            <!-- Butoni i dark mode-->
            <label for="switch">
            <input onclick="lol()" type="checkbox" name="checkbox" class="switch">
            </label>
            <!-- Butoni i animacionit te navigimit responsive-->
            <div class="hamburger">
               <span class="bar"></span>
               <span class="bar"></span>
               <span class="bar"></span>
            </div>
         </nav>
      </header>
      <!-- Navigimi (End)-->

      <!-- Background me Particles JS (Start)-->
      <section class="home-bkg h-100vh" id="home">
         <div class="line-1 anim-typewriter">
            <h1>Sticking Together With A Source Of Quality</h1>
         </div>
         <canvas class="particles-js-canvas-el" style="width: 100%; height: 100%; position: absolute; top: 0px;"></canvas>
      </section>
      <!-- Background me Particles JS (End)-->

      <!-- Form (start) -->
      <div class="form-section">
         <div class="form">
            <form>
               <h1>Get in touch</h1>
               <label for="fname"></label>
               <input type="text" name="fname" placeholder="First Name" required>
               <label for="lname"></label>
               <input type="text" name="lname" placeholder="Last Name" required>
               <label for="email"></label>
               <input type="email" name="email" placeholder="Email" required>
               <textarea name="message" placeholder="Write a message..."></textarea>
               <input type="submit" value="Submit">
            </form>
         </div>
      </div>
      <!-- Form (end) -->

      <!-- Footer (start) -->
      <footer class="footer footer-contact">
         <img src="images/mda.png" alt="logo" class="footer-logo">
         <section class="bottom-nav">
            <a href="/" rel="noopener">Home</a>
            <a href="courses" rel="noopener">Courses</a>
            <a href="contact" rel="noopener">Contact Us</a>
            <a href="signup" rel="noopener">Sign Up</a>
         </section>

         <!-- Rrjetet sociale -->
         <section class="social">
            <a href="https://github.com/MrMothDevs" target="_blank" title="Github/MrMothDevs" rel="noopener"><i class="fab fa-github"></i></a>
            <a href="https://www.youtube.com/channel/UCnPiMm-Jp4P5B2dy7SstDjA" target="_blank" title="Youtube/MrMoth" rel="noopener"><i class="fab fa-youtube"></i></a>
            <i class="fab fa-twitter"></i>
            <a href="https://discord.io/remixor" target="_blank" rel="noopener"><i class="fab fa-discord" title="Discord Server/Remixor"></i></a>
         </section>
